################################################################
#                       Preamble
################################################################

# This is the manually designed schema of YAGO 4.5.1
# CC-BY The YAGO Team
# Do not overwrite this file.
#
# Each class defined here will automatically draw in its subclasses from Wikidata.
# 
# Therefore, classes should be defined here only if
# they have 
# (1) a link to Wikidata and
# (2) properties in which they appear as domain or range 

# TODO: Add more Wikidata properties from https://w.wiki/GW5U
# and from https://www.wikidata.org/wiki/Wikidata:Database_reports/List_of_properties/1-1000

################################################################
#                Top level classes
################################################################

ys:
	rdfs:label "Manual YAGO 4.5 shapes declaration"@en .

# Disjointness constraints
schema:CreativeWork a rdfs:Class .
schema:Event owl:disjointWith 
    schema:CreativeWork.
schema:Organization owl:disjointWith 
    schema:CreativeWork,
    schema:Event.
schema:Taxon owl:disjointWith 
    schema:CreativeWork,
    schema:Event,
    schema:Organization.
schema:Person owl:disjointWith 
    schema:CreativeWork,
    schema:Event,
	schema:Taxon,
    schema:Organization.
schema:Place owl:disjointWith 
    # schema:CreativeWork, # Fabian: removed 2026-01-16 to cater to statues
    schema:Event,
	schema:Person,
    schema:Taxon. 
schema:Product owl:disjointWith
    schema:Event,
    schema:Organization,
    schema:Taxon,
	schema:Person,
    schema:Place.	
schema:Intangible  owl:disjointWith
    schema:Event,
    schema:Organization,
    schema:Taxon,
	schema:Person,
    schema:Place.	

# Top-level class of all individuals
# (excluding literals, classes, properties, etc.)
	
schema:Thing a sh:NodeShape ;
    # ys:fromClass wd:Q35120 ; No mapping!
	sh:property [
        sh:path schema:image ;
        sh:datatype xsd:anyURI ;
        ys:fromProperty wdt:P18 ;
		rdfs:label "an image for the thing"@en ;
    ] ;	
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel, wdt:P225, wdt:P1843 ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		sh:minCount 1;
		ys:fromProperty schema:name, wdt:P2561 ;
    ] ;
    sh:property [
        sh:path schema:mainEntityOfPage ;
        sh:datatype xsd:anyURI ;
		# Fabian: added just so that the checker does not signal an error
		ys:fromProperty ys:willBeFilledFromSchemaAbout ; 		
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
        ys:fromProperty wdt:P854, wdt:P856, wdt:P973, wdt:P2699 ; # Thomas: added other URL properties 2026-01-16
    ] ;
	# We map to Wikidata, Freebase, Google knowledge graph, WordNet, and IMDB.
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P646, wdt:P345, wdt:P2671, wdt:P1053, wdt:P8814;
    ] .

# Class of all classes. Classes can have properties, too, but only annotation properties.
rdfs:Class a sh:NodeShape ;    
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		ys:fromProperty schema:name, wdt:2561 ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P646, wdt:P345, wdt:P2671, wdt:P1053, wdt:P8814;
    ] ;
    sh:property [
        sh:path schema:mainEntityOfPage ;
        sh:datatype xsd:anyURI ;
		# Fabian: added just so that the checker does not signal an error
		ys:fromProperty ys:willBeFilledFromSchemaAbout ; 		
    ] . 

# Class of all properties. Properties can have only annotation properties.
rdf:Property a sh:NodeShape ;
    ys:fromClass wd:Q18616576 ; 
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel, schema:alternateName ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description, rdfs:comment ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		ys:fromProperty schema:name, rdfs:label ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P1628, schema:sameAs ;
    ] .
    
################################################################
#                Intangibles
################################################################

schema:Intangible a rdfs:Class, sh:NodeShape;
	rdfs:subClassOf schema:Thing .

# Awards are needed as domain of schema:award
yago:Award a sh:NodeShape ;
    ys:fromClass wd:Q618779, wd:Q38033430 ;
    rdfs:subClassOf schema:Intangible ;
    sh:property [
        sh:path yago:conferredBy ; 
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P1027 ;
    ] .

# Religions etc. are intangibles
yago:BeliefSystem a sh:NodeShape ;
	rdfs:subClassOf schema:Intangible ;
    ys:fromClass wd:Q5390013 .
	#; They are classes and cannot have properties
	#sh:property [
    #    sh:path yago:foundationalText ; 
    #    sh:class schema:CreativeWork ;
    #    ys:fromProperty wdt:P547 ;
    #] ;
    #sh:property [
    #    sh:path schema:founder ;
    #    sh:class schema:Person ;
    #    ys:fromProperty wdt:P112 ;
    #] .

# We need genders as objects of yago:gender
yago:Gender a sh:NodeShape ;
    ys:fromClass wd:Q48264 ;
    rdfs:subClassOf schema:Intangible .

# Needed as range of the property schema:inLanguage
schema:Language a sh:NodeShape ;
	rdfs:subClassOf schema:Intangible ;
    ys:fromClass wd:Q34770 .

