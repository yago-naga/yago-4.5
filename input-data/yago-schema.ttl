@prefix geo: <http://www.opengis.net/ont/geosparql> .
@prefix owl: <http://www.w3.org/2002/07/owl> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl> .
@prefix skos: <http://www.w3.org/2004/02/skos/core> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .
@prefix wpq: <http://www.wikidata.org/prop/qualifier/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix yago: <http://yago-knowledge.org/resource/> .
@prefix ys: <http://yago-knowledge.org/schema> .

################################################################
#                       Preamble
################################################################

# This is the manually designed schema of YAGO 4.5.1
# CC-BY The YAGO Team
# Do not overwrite this file.
#
# Each class defined here will automatically draw in its subclasses from Wikidata.
# 
# Therefore, classes should be defined here only if
# they have 
# (1) a link to Wikidata and
# (2) properties in which they appear as domain or range 

# TODO: Add more Wikidata properties from https://w.wiki/GW5U
# and from https://www.wikidata.org/wiki/Wikidata:Database_reports/List_of_properties/1-1000

################################################################
#                Top level classes
################################################################

ys:
	rdfs:label "Manual YAGO 4.5 shapes declaration"@en .

# Disjointness constraints
schema:CreativeWork a rdfs:Class .
schema:Event owl:disjointWith 
    schema:CreativeWork.
schema:Organization owl:disjointWith 
    schema:CreativeWork,
    schema:Event.
schema:Taxon owl:disjointWith 
    schema:CreativeWork,
    schema:Event,
    schema:Organization.
schema:Person owl:disjointWith 
    schema:CreativeWork,
    schema:Event,
	schema:Taxon,
    schema:Organization.
schema:Place owl:disjointWith 
    # schema:CreativeWork, # Fabian: removed 2026-01-16 to cater to monuments
    schema:Event,
	schema:Person,
    schema:Taxon. 
schema:Product owl:disjointWith
    schema:Event,
    schema:Organization,
    schema:Taxon,
	schema:Person,
    schema:Place.	
schema:Intangible  owl:disjointWith
    schema:Event,
    schema:Organization,
    schema:Taxon,
	schema:Person,
    schema:Place.	

# Top-level class of all individuals
# (excluding literals, classes, properties, etc.)
	
schema:Thing a sh:NodeShape ;
    # ys:fromClass wd:Q35120 ; No mapping!
	sh:property [
        sh:path schema:image ;
        sh:datatype xsd:anyURI ;
        ys:fromProperty wdt:P18 ;
		rdfs:label "an image for the thing"@en ;
        rdfs:comment "An image of the entity"@en ;
    ] ;	
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel, wdt:P225, wdt:P1843, wdt:P1448, wdt:P1559, wdt:P1477 ;
        rdfs:comment "An alternative name for the entity"@en ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description ;
        rdfs:comment "Provides a comment or description of an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		sh:minCount 1;
		ys:fromProperty schema:name, wdt:P2561, wdt:P1476 ;
        rdfs:comment "Provides a human-readable label for an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
        ys:fromProperty wdt:P854, wdt:P856, wdt:P973, wdt:P2699 ; # Thomas: added other URL properties 2026-01-16
        rdfs:comment "A URL associated with the entity"@en ;
    ] ;
	# We map to Wikidata, Freebase, Google knowledge graph, WordNet, and IMDB.
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P646, wdt:P345, wdt:P2671, wdt:P1053, wdt:P8814;
        rdfs:comment "An identifier of the same entity in another knowledge base"@en ;
   ] 
    .

# Class of all classes. Classes can have properties, too, but only annotation properties.
rdfs:Class a sh:NodeShape ;  
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel ;
        rdfs:comment "An alternative name for the entity"@en ;
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description ;
        rdfs:comment "Provides a comment or description of an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		ys:fromProperty schema:name, wdt:P2561 ;
        rdfs:comment "Provides a human-readable label for an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P646, wdt:P345, wdt:P2671, wdt:P1053, wdt:P8814;
        rdfs:comment "An identifier of the same entity in another knowledge base"@en ;
    ] ;
    sh:property [
        sh:path schema:url ;
        sh:datatype xsd:anyURI ;
		ys:fromProperty wdt:P854, wdt:P856, wdt:P973, wdt:P2699; 
        rdfs:comment "A URL of a page about the entity"@en ;		
    ] . 

# Class of all properties. Properties can have only annotation properties.
rdf:Property a sh:NodeShape ;
    ys:fromClass wd:Q18616576 ; 
    sh:property [
        sh:path schema:alternateName ;
        sh:datatype rdf:langString ;
        ys:fromProperty skos:altLabel, schema:alternateName ;
        rdfs:comment "An alternative name for the entity"@en ;

    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        ys:fromProperty schema:description, rdfs:comment ;
        rdfs:comment "Provides a comment or description of an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
		ys:fromProperty schema:name, rdfs:label ;
        rdfs:comment "Provides a human-readable label for an entity in a specific language"@en ;
    ] ;
    sh:property [
        sh:path schema:sameAs ;
		sh:datatype xsd:string ;
        ys:fromProperty wdt:P1628, schema:sameAs ;
        rdfs:comment "An identifier of the same entity in another knowledge base" 
    ] .
    
################################################################
#                Intangibles
################################################################

schema:Intangible a rdfs:Class, sh:NodeShape;
	rdfs:subClassOf schema:Thing .

# Awards are needed as domain of schema:award
yago:Award a sh:NodeShape ;
    ys:fromClass wd:Q618779, wd:Q38033430 ;
    rdfs:subClassOf schema:Intangible ;
    sh:property [
        sh:path yago:conferredBy ; 
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P1027 ;
        rdfs:comment "The organization or person that conferred an award"@en ;
    ] .

# Religions etc. are intangibles
yago:BeliefSystem a sh:NodeShape ;
	rdfs:subClassOf schema:Intangible ;
    ys:fromClass wd:Q5390013 .
	#; They are classes and cannot have properties
	#sh:property [
    #    sh:path yago:foundationalText ; 
    #    sh:class schema:CreativeWork ;
    #    ys:fromProperty wdt:P547 ;
    #] ;
    #sh:property [
    #    sh:path schema:founder ;
    #    sh:class schema:Person ;
    #    ys:fromProperty wdt:P112 ;
    #] .

# We need genders as objects of yago:gender
yago:Gender a sh:NodeShape ;
    ys:fromClass wd:Q48264 ;
    rdfs:subClassOf schema:Intangible .

# Needed as range of the property schema:inLanguage
schema:Language a sh:NodeShape ;
	rdfs:subClassOf schema:Intangible ;
    ys:fromClass wd:Q34770 .

# For units of measurement for quantified properties such as "width"
yago:UnitOfMeasurement a sh:NodeShape ;
	rdfs:subClassOf schema:Intangible ;
    ys:fromClass wd:Q47574 .
	
################################################################
#                Media
################################################################

schema:CreativeWork a sh:NodeShape ;
    ys:fromClass wd:Q17537576, wd:Q386724 ;
	rdfs:subClassOf schema:Thing ;
    sh:property [
        sh:path schema:about ;
        sh:class schema:Thing ;
        ys:fromProperty wdt:P921 ;
        rdfs:comment "Indicates what a CreativeWork is about"@en ;
    ] ;
    sh:property [
        sh:path schema:award ;
        sh:class yago:Award ;
        ys:fromProperty wdt:P166 ;
        rdfs:comment "An award won by a Person, Organization, CreativeWork, or Product"@en ;
    ] ;
    sh:property [
        sh:path schema:author ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P50, wdt:P170 ; 
        rdfs:comment "The author of a CreativeWork or FictionalEntity"@en ;
    ] ;
    sh:property [
        sh:path schema:contentLocation ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P840 ;
        rdfs:comment "The location depicted or described in the CreativeWork"@en ;
    ] ;
    sh:property [
        sh:path schema:dateCreated ;
        sh:or (
            [
                sh:datatype xsd:dateTime
            ]
            [
                sh:datatype xsd:date
            ]
            [
                sh:datatype xsd:gYearMonth
            ]
            [
                sh:datatype xsd:gYear
            ]
        ) ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P571 ;
        rdfs:comment "The date something was created (used for CreativeWorks, Organizations, Products, AdministrativeAreas, etc.)"@en ;
    ] ;
    sh:property [
        sh:path schema:inLanguage ;
        sh:class schema:Language ;
        ys:fromProperty wdt:P407 ;
        rdfs:comment "The language of a CreativeWork"@en ;
    ] .
    
schema:Book a sh:NodeShape ;
	rdfs:subClassOf schema:CreativeWork ;
    ys:fromClass wd:Q571 ;
    sh:property [
        sh:path schema:isbn ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P212 , wdt:P957 ;
        sh:pattern "^(97[89]-([0-57]-(\\d-\\d{7}|\\d\\d-\\d{6}|\\d\\d\\d-\\d{5}|\\d{4}-\\d{4}|\\d{5}-\\d\\d\\d|\\d{6}-\\d\\d|\\d{7}-\\d)|[89]\\d-(\\d-\\d{6}|\\d\\d-\\d{5}|\\d\\d\\d-\\d{4}|\\d{4}-\\d\\d\\d|\\d{5}-\\d\\d|\\d{6}-\\d)|[69]\\d\\d-(\\d-\\d{5}|\\d\\d-\\d{4}|\\d\\d\\d-\\d\\d\\d|\\d{4}-\\d\\d|\\d{5}-\\d)|99[0-8]\\d-\\d-\\d{4}|99[0-8]\\d-\\d\\d-\\d\\d\\d|99[0-8]\\d-\\d\\d\\d-\\d\\d|99[0-8]\\d-\\d{4}-\\d|999\\d\\d-\\d-\\d\\d\\d|999\\d\\d-\\d\\d-\\d\\d|999\\d\\d-\\d\\d\\d-\\d)-\\d|\\d{9}(\\d|X))|(\\d{1,5}-\\d{1,7}-\\d{1,6}-[0-9X])$" ;
        rdfs:comment "The ISBN (International Standard Book Number) of a Book"@en ;
    ] ;
	sh:property [
        sh:path schema:illustrator ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P110 ;
        rdfs:comment "The illustrator of a Book"@en ;
    ] ;
    sh:property [
        sh:path schema:editor ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P98 ;
        rdfs:comment "An editor of a Book"@en ;
    ] ;	
    sh:property [
        sh:path schema:publisher ;
		sh:maxCount 1 ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P123 ;
        rdfs:comment "The publisher of a Book or Newspaper"@en ;
    ] ;	
    sh:property [
        sh:path schema:numberOfPages ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P1104 ;
        rdfs:comment "The number of pages in a Book"@en ;
    ] .

schema:Newspaper a sh:NodeShape ;
	rdfs:subClassOf schema:CreativeWork ; # instead of series
    ys:fromClass wd:Q11032 ;
    sh:property [
        sh:path schema:publisher ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
		sh:maxCount 1;
        ys:fromProperty wdt:P123 ;
        rdfs:comment "The publisher of a Book or Newspaper"@en ;
    ] ;
    sh:property [
        sh:path schema:sponsor ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P859 ;
        rdfs:comment "The sponsor of an Event or Newspaper"@en ;
    ] .

schema:TVSeries a sh:NodeShape ;
	rdfs:subClassOf schema:CreativeWork ; # instead of series
    ys:fromClass wd:Q5398426 ;
    sh:property [
        sh:path schema:musicBy ; 
        sh:or (
            [
                sh:class schema:MusicGroup
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P86 ;
        rdfs:comment "The composer or musician for a Movie, TV Series, or MusicComposition"@en ;
    ] ;		
    sh:property [
        sh:path schema:actor ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P161 ;
        rdfs:comment "Indicates an actor who appeared in a Movie or TV Series"@en ;
    ] ;
    sh:property [
        sh:path schema:locationCreated ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P495 ;
        rdfs:comment "The location where a Movie, TV Series, Organization, or HumanMadeGeographicalEntity was created"@en ;

    ] ;
    sh:property [
        sh:path schema:director ;
        sh:class schema:Person ;
		ys:fromProperty wdt:P57 ;
        rdfs:comment "The director of a Movie, TV Series, or PerformingGroup"@en ;
    ] ;
    sh:property [
        sh:path schema:numberOfEpisodes ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P1113 ;
        rdfs:comment "The number of episodes in a TV Series"@en ;
    ] ;
    sh:property [
        sh:path schema:numberOfSeasons ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P2437 ;
        rdfs:comment "The number of seasons in a TV Series"@en ;
    ] ;   
    sh:property [
        sh:path schema:productionCompany ;
        sh:class schema:Organization ;
        ys:fromProperty wdt:P272 ;
        rdfs:comment "The production company for a Movie or TV Series"@en ;
    ] ;
    #Margherita 18/01/2026 added new property
    sh:property [
        sh:path yago:directorOfPhotography ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P344 ;
        rdfs:comment "The director of photography for a Movie or TV Series"@en ;
    ] .


schema:MusicComposition a sh:NodeShape ;
	rdfs:subClassOf schema:CreativeWork ;
    ys:fromClass wd:Q207628, wd:Q2188189 ;
    sh:property [
        sh:path schema:musicBy ; # Fabian: cannot map to "composer", because a Wikidata property always has to go to the same schema property
        sh:or (
            [
                sh:class schema:Person
            ]
            [
                sh:class schema:MusicGroup
            ]
        ) ;
        ys:fromProperty wdt:P86 ;
        rdfs:comment "The composer or musician for a Movie, TV Series, or MusicComposition"@en ;
    ] ;
    sh:property [
        sh:path yago:iswcCode ; # Fabian: Changed from schema: to yago: 2024-04-03
        sh:datatype xsd:string ;
        sh:pattern "^T-[0-9]{3}\\.[0-9]{3}\\.[0-9]{3}-[0-9]$" ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P1827 ;
    ] ;
    sh:property [
        sh:path schema:lyricist ;
        sh:or (
            [
                sh:class schema:Person
            ]
            [
                sh:class schema:MusicGroup
            ]
        ) ;
        ys:fromProperty wdt:P676 ;
        rdfs:comment "The lyricist of a MusicComposition"@en ;
    ] .

schema:Movie a sh:NodeShape ;
	rdfs:subClassOf schema:CreativeWork ;
    ys:fromClass wd:Q11424 ; 
    sh:property [
        sh:path schema:actor ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P161 ;
        rdfs:comment "Indicates an actor who appeared in a Movie or TV Series"@en ;

    ] ;
    sh:property [
        sh:path schema:locationCreated ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P495 ;
        rdfs:comment "The location where a Movie, TV Series, Organization, or HumanMadeGeographicalEntity was created"@en ;
    ] ;
    sh:property [
        sh:path schema:musicBy ; 
        sh:or (
            [
                sh:class schema:MusicGroup
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P86 ;
        rdfs:comment "The composer or musician for a Movie, TV Series, or MusicComposition"@en ;
    ] ;	
    sh:property [
        sh:path schema:director ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P57 ;
        rdfs:comment "The director of a Movie, TV Series, or PerformingGroup"@en ;
    ] ;
    sh:property [
        sh:path schema:duration ;
		sh:maxCount 1 ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2047 ;
        rdfs:comment "The duration of a Movie (typically in minutes)"@en ;
    ] ;
    sh:property [
        sh:path schema:productionCompany ;
        sh:class schema:Organization ;
        ys:fromProperty wdt:P272 ;
        rdfs:comment "The production company for a Movie or TV Series"@en ;
    ] ;
    #Margherita 18/01/2026: added property 
    sh:property [
        sh:path yago:directorOfPhotography ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P344 ;
        rdfs:comment "The director of photography for a Movie or TV Series"@en ;
    ] .

# Fictional entities are Things,
# from  fictional entity (Q14897293) ,  imaginary character (Q115537581) ,  hypothetical entity (Q18706315) ,
#  character that may or may not be fictional (Q21070598),  fictional creature (Q2593744) ,
#  fictional character (Q95074),  non-existent entity (Q64728693),  fictional object (Q15706911)  
yago:FictionalEntity a sh:NodeShape ;
	rdfs:subClassOf schema:Thing ;
	rdfs:label "Fictional entity"@en ;
	sh:property [
        sh:path yago:appearsIn ;
        sh:class schema:CreativeWork ;
        ys:fromProperty wdt:P1080, wdt:P1441, wdt:P4584 ;
        rdfs:comment "A CreativeWork in which a FictionalEntity appears"@en ;
    ] ;
	sh:property [
        sh:path schema:performer ;
        sh:class schema:Person ;
        ys:fromProperty wdt:P175  ;
        rdfs:comment "A Person who performs as a FictionalEntity"@en ;
    ] ;
	sh:property [
        sh:path schema:author ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P170, wdt:P50 ;
        rdfs:comment "The author of a CreativeWork or FictionalEntity"@en ;
    ] ;
	ys:fromClass wd:Q14897293, wd:Q115537581, wd:Q18706315, wd:Q21070598, wd:Q64728693 .

################################################################
#                Events
################################################################


schema:Event a sh:NodeShape ;
    # Merge "occurrence" and "event", also to avoid having "event" twice in YAGO
    ys:fromClass wd:Q1190554, wd:Q1656682 ;
	rdfs:subClassOf schema:Thing ;
    sh:property [
        sh:path schema:about ;
        sh:class schema:Thing ;
        ys:fromProperty wdt:P921 ;
        rdfs:comment "Indicates what a CreativeWork or Event is about"@en ;
    ] ;
	sh:property [
        sh:path schema:location ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P131 , wdt:P276 , wdt:P706 , wdt:P4552, wdt:P17 ;
        rdfs:comment "The location of an Organization, Event, or Place"@en ;
    ] ;
    sh:property [
        sh:path schema:organizer ;
        sh:or (
            [
                sh:class schema:Person
            ]
            [			
                sh:class schema:Organization
            ]
        ) ;
        ys:fromProperty wdt:P664 ;
        rdfs:comment "The organizer of an Event"@en ;
    ] ;
    sh:property [
        sh:path schema:startDate ;
        sh:or (
            [
                sh:datatype xsd:dateTime
            ]
            [
                sh:datatype xsd:date
            ]
            [
                sh:datatype xsd:gYearMonth
            ]
            [
                sh:datatype xsd:gYear
            ]
        ) ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P580 ;
        rdfs:comment "The start date of an Event"@en ;
    ] ;
    sh:property [
        sh:path schema:endDate ;
        sh:or (
            [
                sh:datatype xsd:dateTime
            ]
            [
                sh:datatype xsd:date
            ]
            [
                sh:datatype xsd:gYearMonth
            ]
            [
                sh:datatype xsd:gYear
            ]
        ) ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P582 ;
        rdfs:comment "The end date of an Event"@en ;
    ] ;
    sh:property [
        sh:path schema:sponsor ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P859 ;
        rdfs:comment "The sponsor of an Event or Newspaper"@en ;
    ] ;
	sh:property [
        sh:path yago:follows ;
        sh:class schema:Event ;
        ys:fromProperty wdt:P155 ;
        rdfs:comment "An Event that follows another Event chronologically"@en ;
    ] ;
	sh:property [
        sh:path yago:participant ;
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P710, wdt:P1923 ;
        rdfs:comment "A Person or Organization that participated in an Event"@en ;
    ] ;
	sh:property [
        sh:path schema:superEvent ;
        sh:class schema:Event ;
        ys:fromProperty wdt:P361 ;
        rdfs:comment "A larger Event that contains this Event"@en ;
    ] ;
	sh:property [
        sh:path schema:maximumAttendeeCapacity ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P1083 ;
	rdfs:comment "The total number of individuals that may attend an event or venue"@en ;	
    ] .

# Fabian 2023-05-04: In Wikidata, elections are subclasses of two disjoint classes, so we attach it under Event here
yago:Election a sh:NodeShape ;
	rdfs:subClassOf schema:Event ;
    ys:fromClass wd:Q40231 .


################################################################
#                Organizations
################################################################


schema:Organization a sh:NodeShape ;
    ys:fromClass wd:Q43229, wd:Q895526, wd:Q167037, wd:Q155076 ; # Fabian: we add corporations here (in the sense of legal entities), and juridical persons, and organs
	rdfs:subClassOf schema:Thing ;
    sh:property [
        sh:path schema:award ;
        sh:class yago:Award ;
        ys:fromProperty wdt:P166 ;
        rdfs:comment "An award won by a Person, Organization, CreativeWork, or Product"@en ;
    ] ;
    sh:property [
        sh:path schema:locationCreated ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P740, wdt:P495 ;
        rdfs:comment "The location where a Movie, TV Series, Organization, or HumanMadeGeographicalEntity was created"@en ;
    ] ;
    sh:property [
        sh:path schema:location ;
