# General superclass for geographic places
# We don't map this to Wikidata, because we want to control the types of places we have in YAGO

schema:Place a sh:NodeShape ;
    # No mapping here, to avoid getting messy subclasses
    # ys:fromClass wd:Q17334923 ; 
	rdfs:subClassOf schema:Thing ;
    sh:property [
        sh:path schema:location ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P131 , wdt:P276 , wdt:P706 , wdt:P4552, wdt:P17 ;
        rdfs:comment "The location of an Organization, Event, or Place"@en ;
    ] ;
    sh:property [
        sh:path yago:area ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2046 ;
    ] ;
    sh:property [
        sh:path yago:highestPoint ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:class schema:Place ;
        ys:fromProperty wdt:P610 ;
        rdfs:comment "The highest point of a Place"@en ;
    ] ;
    sh:property [
        sh:path yago:lowestPoint ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:class schema:Place ;
        ys:fromProperty wdt:P1589 ;
        rdfs:comment "The lowest point of a Place"@en ;
    ] ;
    sh:property [
        sh:path yago:neighbors ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:class schema:Place ;
        ys:fromProperty wdt:P47, wdt:P206 ;
        rdfs:comment "A neighboring Place"@en ;
    ] ;
    sh:property [
        sh:path yago:elevation ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2044 ;
    ] ;
    sh:property [
        sh:path schema:geo ;
        sh:datatype geo:wktLiteral ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P625 ;
        rdfs:comment "The geographic coordinates of a Place (in WKT literal format)"@en ;
    ] ;
# Fabian: would need proper modeling of the target class	
#    sh:property [
#        sh:path yago:flag ; # Thomas: added to yago: 2026-01-16
#        sh:class schema:CreativeWork ;
#        ys:fromProperty wdt:P163 ;
	#rdfs:comment "Flag of the place or organization."@en ;
#    ] ;
    # Margherita 18/01/2026: added time zone property (P421)
	# Fabian: TODO this would need proper modeling with timeZone as a new class (subclass of Place?)
    sh:property [
        sh:path schema:timeZone ;
        sh:datatype xsd:string ;
        ys:fromProperty wdt:P421 ;
        rdfs:comment "The time zone where a Place is located."@en ;
    ] .


# ------------------ Planets ---------------

# One permissible subclass of schema:Place is yago:AstronomicalObject
# We import all subclasses from Wikidata
yago:AstronomicalObject a sh:NodeShape ;
	rdfs:subClassOf schema:Place ;
	sh:property [
        sh:path yago:parentBody ;
        sh:class yago:AstronomicalObject ;
        ys:fromProperty wdt:P397 ;
        rdfs:comment "The parent celestial body of an AstronomicalObject"@en ;
    ] ;    
	sh:property [
        sh:path yago:mass ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2067 ;
        rdfs:comment "The mass of an AstronomicalObject"@en ;
    ] ;    
	sh:property [
        sh:path yago:luminosity ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2060 ;
        rdfs:comment "The luminosity of an AstronomicalObject"@en ;
    ] ;
    sh:property [
        sh:path yago:parallax  ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2214 ;
        rdfs:comment "The parallax measurement of an AstronomicalObject"@en ;
    ] ;    
	sh:property [
        sh:path yago:radialVelocity  ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2216 ;
        rdfs:comment "The radial velocity of an AstronomicalObject"@en ;
    ] ;    
	sh:property [
        sh:path yago:distanceFromEarth ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P2583 ;
        rdfs:comment "The distance of an AstronomicalObject from Earth"@en ;
    ] ;    
    # Margherita 25/01/2026: now the wikidata prop should be correct, 
    # for the target class, this prop in wikidata is quit polysemic, it is used as a numeric astronomical reference epoch (e.g., J2000.0) for fixed celestial objects, and as a temporal reference date for orbital elements of moving objects
    sh:property [
        sh:path yago:epoch ;
        sh:or (
            [
            sh:datatype xsd:string ;
            #sh:pattern "^[JB][0-9]{4}(\\.[0-9]+)?$" ; # Fabian: We can't check patterns in a disjunction :-(
            ]
            [
            sh:datatype xsd:date ;
            ]
        ) ;
		rdfs:comment "Astronomical reference epoch (e.g., J2000.0)" ;
        ys:fromProperty wdt:P6259 ;
    ] ;
	ys:fromClass wd:Q6999.

