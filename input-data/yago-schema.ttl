# --------------------- Land Forms ------------------------

# One permissible subclass of schema:Place are land forms,
# from terrain, landmass, Landform, geographical area, natural geographic object
# not from geographical region (Q82794) and geographic location  (Q2221906) because otherwise many administrative regions become Landforms
schema:Landform a sh:NodeShape ;
    rdfs:subClassOf schema:Place ;
	ys:fromClass wd:Q14524493, wd:Q205895, wd:Q271669, wd:Q3622002, wd:Q35145263 .
    
# Bodies of water are Landforms.
# Linked to body of water and marine body of water
schema:BodyOfWater a sh:NodeShape ;
	rdfs:subClassOf schema:Landform ;
	sh:property [
        sh:path yago:flowsInto ;
        sh:class schema:BodyOfWater ;
        ys:fromProperty wdt:P403, wdt:P201 ;
        rdfs:comment "The BodyOfWater that another body of water flows into"@en ;
    ] ;
    ys:fromClass wd:Q15324, wd:Q116126039 .

# Continents
schema:Continent a sh:NodeShape ;
    rdfs:subClassOf schema:Landform ;
    ys:fromClass wd:Q5107 .


# -------------------- Human-made Places --------------------

# One permissible subclass of schema:Place are human made places.
# Mapped to "architectural structure" and Human Made Feature (twice...) in Wikidata.
# We thus import all subclasses from Wikidata here, 
# and need to define classes only to define properties
yago:HumanMadeGeographicalEntity a sh:NodeShape ;
	rdfs:subClassOf schema:Place ;
	rdfs:label "Human-made geographical entity"@en ;
    ys:fromClass wd:Q811979, wd:Q811430, wd:Q811463, wd:Q35145743, wd:Q811430 ;
	sh:property [
        sh:path yago:ownedBy ; # Fabian: changed from schema: to yago: 2024-04-03
        sh:or (
            [
                sh:class schema:Organization
            ]
            [
                sh:class schema:Person
            ]
        ) ;
        ys:fromProperty wdt:P127, wdt:P749 ;
        rdfs:comment "The Organization or Person that owns an Organization or HumanMadeGeographicalEntity"@en ;
    ] ;	
	sh:property [
        sh:path schema:dateCreated ;
        sh:datatype xsd:dateTime ;
        ys:fromProperty wdt:P571 ;
		sh:maxCount 1 ;
        rdfs:comment "The date something was created (used for CreativeWorks, Organizations, Products, AdministrativeAreas, etc.)"@en ;
    ] ;
	sh:property [
        sh:path schema:maximumAttendeeCapacity ;
        sh:datatype xsd:decimal ;
        ys:fromProperty wdt:P1083 ;
	rdfs:comment "The total number of individuals that can contain this place."@en ;	
    ] .

# Transit routes such as roads and railways.
# Mapped to "thoroughfare" Wikidata (not a subclass of arch struc), 
# not existing in schema.org
yago:Way a sh:NodeShape ;
	rdfs:label "Way"@en ;	
	rdfs:subClassOf yago:HumanMadeGeographicalEntity ;
    ys:fromClass wd:Q83620 ;
	sh:property [
        sh:path yago:terminus ;
        sh:class schema:Place ;
        ys:fromProperty wdt:P609 ;
        rdfs:comment "A Place where a Way terminates"@en ;
    ] ;
	sh:property [
        sh:path yago:length ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P2043 ;
        rdfs:comment "The length of a Way (e.g., road, river)"@en ;
    ] .

# Listed here to add properties, and attached to yago:HumanMadeGeographicalEntity to avoid schema:CivicStructure
schema:Airport a sh:NodeShape ;
    rdfs:subClassOf yago:HumanMadeGeographicalEntity ;
    ys:fromClass wd:Q1248784 ;
    sh:property [
        sh:path schema:iataCode ;
        sh:datatype xsd:string ;
		sh:pattern "^[A-Z0-9]{3}$" ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P238 ;
        rdfs:comment "The IATA code for an Airport or Airline"@en ;
    ] .

################################################################
#                Commercial Products
################################################################

# An individual product with a GTIN. 
# Mapping also Collectables and Goods
schema:Product a sh:NodeShape ;
	rdfs:subClassOf schema:Thing ;
    ys:fromClass wd:Q2424752, wd:Q2342494, wd:Q28877, wd:Q2184715 ;
    sh:property [
        sh:path schema:award ;
        sh:class yago:Award ;
        ys:fromProperty wdt:P166 ;
        rdfs:comment "An award won by a Person, Organization, CreativeWork, or Product"@en ;
    ] ;
    sh:property [
        sh:path schema:material ;
        sh:class schema:Product ;
        ys:fromProperty wdt:P186 ;
        rdfs:comment "The material a Product is made of"@en ;
    ] ;
    sh:property [
        sh:path schema:manufacturer ;
        sh:class schema:Corporation ;
        ys:fromProperty wdt:P176, wdt:P178 ;
        rdfs:comment "The manufacturer of a Product"@en ;
    ] ;
    sh:property [
        sh:path schema:dateCreated ;
        sh:or (
            [
                sh:datatype xsd:dateTime
            ]
            [
                sh:datatype xsd:date
            ]
            [
                sh:datatype xsd:gYearMonth
            ]
            [
                sh:datatype xsd:gYear
            ]
        ) ;
        sh:maxCount 1 ;
        ys:fromProperty wdt:P577 ;
        rdfs:comment "The date something was created (used for CreativeWorks, Organizations, Products, AdministrativeAreas, etc.)"@en ;
    ] ;	
    # Margherita 26/01/2026: in wikidata there is the numerical amount followed by the unit ( 73 cm)
	sh:property [
        sh:path schema:width ;
        sh:datatype yago:UnitOfMeasurement ;
        ys:fromProperty wdt:P2049 ;
        rdfs:comment "The width of an object."@en ;
    ] ;
    # Margherita 26/01/2026: in wikidata there is the numerical amount followed by the unit ( 73 cm)
    sh:property [
        sh:path schema:height ;
        sh:datatype yago:UnitOfMeasurement ;
        ys:fromProperty wdt:P2048 ;
        rdfs:comment "The height of an object."@en ;
    ] ;
    sh:property [
        sh:path schema:gtin ;
        sh:datatype xsd:string ;
        sh:pattern "^\\d{12,14}|\\d{8}$" ;
        ys:fromProperty wdt:P3962 ;
        rdfs:comment "The Global Trade Item Number for a Product"@en ;
    ] .

################################################################
#                Animals and other organisms
################################################################

# We model every taxon not as a class, but as an instance.
# This is the only way in which we can make assertions about taxons
schema:Taxon a sh:NodeShape;
	rdfs:subClassOf schema:Thing ;
	sh:property [
        sh:path schema:parentTaxon ;
        sh:class schema:Taxon ;
        ys:fromProperty wdt:P171 ;
        rdfs:comment "The parent taxon of a Taxon in biological classification"@en ;
    ] ;
	sh:property [
        sh:path yago:consumes ;
        sh:class schema:Taxon ;
        ys:fromProperty wdt:P1034 ;
        rdfs:comment "A Taxon that another Taxon consumes (predator-prey relationship)"@en ;
    ] ;
	ys:fromClass wd:Q16521 .
	
################################################################
#                Time annotation
################################################################

ys:AnnotationPropertyShape a rdfs:Property ;
    rdfs:subPropertyOf sh:PropertyShape .

ys:startDateAnnotation a ys:AnnotationPropertyShape ;
    sh:path schema:startDate ;
    sh:or (
        [
            sh:datatype xsd:dateTime
        ]
        [
            sh:datatype xsd:date
        ]
        [
            sh:datatype xsd:gYearMonth
        ]
        [
            sh:datatype xsd:gYear
        ]
    ) ;
    sh:maxCount 1 ;
    ys:fromProperty wpq:P580 ;
    rdfs:comment "The start date of an Event"@en .

ys:endDateAnnotation a ys:AnnotationPropertyShape ;
    sh:path schema:endDate ;
    sh:or (
        [
            sh:datatype xsd:dateTime
        ]
        [
            sh:datatype xsd:date
        ]
        [
            sh:datatype xsd:gYearMonth
        ]
        [
            sh:datatype xsd:gYear
        ]
    ) ;
    sh:maxCount 1 ;
    ys:fromProperty wpq:P582 ;
    rdfs:comment "The end date of an Event"@en .
